        "properties": {
          "password": {
            "type": "string"
          },
          "passwordConfirm": {
            "type": "string"
          }
        }
      },
      "body_request_change_password": {
        "type": "object",
        "required": true,
        "properties": {
          "email": {
            "type": "string"
          }
        }
      },
      "body_settings": {
