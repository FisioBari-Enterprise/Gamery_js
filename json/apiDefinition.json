          "volume": {
            "type": "number"
          },
          "sound": {
            "type": "boolean"
          }
        }
      },
      "body_change_country": {
        "type": "object",
        "required": true,
        "properties": {
          "id": {
            "type": "string",
            "required": false
          },
          "code": {
            "type": "string",
            "required": false
          }
        }
      },
      "body_game_round": {
        "type": "object",
        "required": true,
        "properties": {
          "words": {
            "type": "array",
            "items": [{
              "type": "string"
            }]
          },
          "game_time": {
            "type": "number"
          }
        }
      },
