        "schemas": {},
        "requestBodies": {
            "client": {
                "login": {
                    "description": "Dati per effettuare il login",
                    "require": true,
                    "content": {
                        "application\\json": {
                            "schema": {
                                "oneOf": {
                                    "temporary": {
                                        "type": "object",
                                        "properties": {
                                            "uuid": {
                                                "type": "string"
                                            }
                                        }
                                    },
                                    "credentials": {
                                        "type": "object",
                                        "properties": {
                                            "usernameEmail": {
                                                "type": "string"
                                            },
                                            "password": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            },
                            "example": {
                                "uuid": "00000000-0000-0000-0000-000000000000"
                            }
                        }
                    }
                },
                "registration": {
                    "description": "Dati per effettuare la registrazione del nuovo utente",
                    "require": true,
                    "content": {
                        "application\\json": {
                            "schema": {
                                "type": "object",
                                "required": true,
                                "properties": {
                                    "uuid": {
                                        "type": "string"
                                    },
                                    "username": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            },
                            "example": {
                                "uuid": "00000000-0000-0000-0000-000000000000",
                                "username": "test",
                                "email": "test@gamery.com",
                                "password": "password"
                            }
                        }
                    }
                }
            },
            "change_password_body": {
                "type": "object",
                "required": true,
                "properties": {
                    "password": {
                        "type": "string"
                    },
                    "passwordConfirm": {
                        "type": "string"
                    }
                }
            },
            "change_password_email_body": {
                "type": "object",
                "required": true,
                "properties": {
                    "email": {
                        "type": "string"
                    }
                }
            },
            "setting_body": {
                "type": "object",
                "required": true,
                "properties": {
                    "font_size": {
                        "type": "number"
                    },
                    "volume": {
                        "type": "number"
                    },
                    "sound": {
                        "type": "boolean"
                    }
                }
            },
            "change_country_body": {
                "type": "object",
                "required": true,
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "code": {
                        "type": "string"
                    }
                }
            },
            "game_body": {
                "type": "object",
                "required": true,
                "properties": {
                    "words": {
                        "type": "array",
                        "item": "string"
                    },
                    "game_time": {
                        "type": "number"
                    }
                }
            }
